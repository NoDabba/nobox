<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE tiles-definitions PUBLIC  
        "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"  
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<definition name="DefaultTemplate" template="/WEB-INF/views/template/SiteTemplate.jsp">
		<put-attribute name="title" value="No DABBA" />
		<put-attribute name="inLineCssStyle"
			value="/WEB-INF/views/template/inlineCssStyle.jsp" />
		<put-attribute name="header" value="/WEB-INF/views/template/header.jsp" />
		<put-attribute name="navigation"
			value="/WEB-INF/views/template/navigation.jsp" />
		<put-attribute name="socialuser"
			value="/WEB-INF/views/template/socialuser.jsp" />
		<put-attribute name="banner"
			value="/WEB-INF/views/template/banner1.jsp" />
		<put-attribute name="body"
			value="/WEB-INF/views/template/emptybody.jsp" />
		<put-attribute name="footer" value="/WEB-INF/views/template/footer.jsp" />
	</definition>

	<definition name="restaurantTemplate" extends="DefaultTemplate">
		<put-attribute name="title" value="No DABBA | Restaurant Details" />
		<put-attribute name="header" value="/WEB-INF/views/template/header2.jsp" />
		<put-attribute name="navigation"
			value="/WEB-INF/views/template/navigation2.jsp" />
		<put-attribute name="inLineCssStyle"
			value="/WEB-INF/views/template/emptybody.jsp" />
		<put-attribute name="socialuser"
			value="/WEB-INF/views/template/emptybody.jsp" />
		<put-attribute name="banner"
			value="/WEB-INF/views/template/banner2.jsp" />			
	</definition>
	
	
</tiles-definitions>